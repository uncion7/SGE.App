@page
@model SGE.App.Frontend.Pages.MunicipioEditModel
@{
}
<h1>Municipio:@Model.Municipio.Nombre</h1>
<form method="post" action="">
    <input type="hidden" asp-for="Municipio.Id">
    <div class="form-group">
        <label asp-for="Municipio.Nombre"></label>
        <input asp-for="Municipio.Nombre" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Municipio.Departamento"></label>
        <input asp-for="Municipio.Departamento" class="form-control">
    </div>
    <div class="col-12">
        <nav class="navbar navbar-light bg-light">
            <a asp-page="./MunicipioList" class="btn btn-primary" data-toggle="tooltip" data-placement="top" title="Listar">
                <i class="fas fa-list-ul"></i>
            </a>
            <button type="submit" class="btn btn-primary" data-toggle="tooltip" data-placement="top" title="Guardar">
                <i class="fas fa-save"></i>        
            </button>
        </div>
    </div>
</form>
